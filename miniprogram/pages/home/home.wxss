/* pages/home/home.wxss */
page{
  background: #fff
}
.menus{
  width: 100%;
  /* background: red */
  flex-wrap: wrap
}
.menusItem{
  width: 20%;
  box-sizing: border-box;
  padding: 20rpx
}
.menusIcon{
   width: 100%;
   /* background: red */
}
.menusText{
  display: block;
  width:100%;
  color:#696969
}
.bannerBox{
  box-sizing: border-box;
  /* padding: 20rpx; */
  /* background: red */
}
.noticeBox{
  background: #fff;
  width: 100%;
  overflow: hidden;
  height: 60rpx;
  line-height: 60rpx;
  box-sizing: border-box;
  padding: 0 30rpx;
  color: rgb(255, 72, 0)
}

.pro_box{
  padding: 20rpx
}
.pro_pic{
  width: 100%;
  height: 300rpx;
  border-radius: 20rpx
}
.pro_title{
  font-size: 32rpx;
  line-height: 1.4
}
.pro_des{
  color: #999;
}
.prizeBox{
  border-bottom: 1rpx solid #d3d3d3;
  color: #3598d2
}
.lastPrize{
  font-size: 60rpx;
  color: #3598d2;
  /* font-family:Impact, Haettenschweiler, 'Arial Narrow Bold', sans-serif */
  font-family:cursive;
  font-weight: 600
}
.delePrize{
  text-decoration-line:line-through;
  font-size: 22rpx;
  color: #999
}